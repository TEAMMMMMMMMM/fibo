#include <iostream>
using namespace std;
int main() {
  int hour,minute;
  cout<<"Please insert hour: ";
  cin>>hour;
  cout<<"Please insert minute: ";
  cin>>minute;
  if(minute>0)
  {
    hour++;
  }
  else if(minute<0 || hour<0)
  {
    cout<<"Please enter positive integer";
  }
  hour--;
  cout<<hour*30<<" Bath";
}
