#include <iostream>
#include <cmath>
using namespace std;
int main() {
  float num;
  cin>>num;
  if(num>0)
  {
    cout<<"positive ";
  }
  else if(num<0)
  {
    cout<<"negative ";
  }
  else
  {
    cout<<"zero";
    return 0;
  }
  if(fmod(num,2)==0)
  {
    cout<<"even";
  }
  else
  {
    cout<<"odd";
  }
}
