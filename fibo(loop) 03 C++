#include <iostream>
#include <random>
using namespace std;
int main()
{
    int random_number = rand() % 100 + 1,guess;
    
    cout<<"Guess number:";
    cin>>guess;
    while(guess!=random_number)
      {
        if(guess>random_number)
        {
          cout<<"Less than this"<<endl;
        }
        else
        {
          cout<<"More than this"<<endl;
        }
        cout<<"Guess number:";
        cin>>guess;
      }
  cout<<"Correct";
}
